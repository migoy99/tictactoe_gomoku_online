{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "asia-east1"
    },
    "rewrites": [
      {
        "source": "/tic-tac-toe/**",
        "destination": "/tic-tac-toe/[id].html"
      },
      {
        "source": "/gomoku/**",
        "destination": "/gomoku/[id].html"
      }
    ]
  }
}
